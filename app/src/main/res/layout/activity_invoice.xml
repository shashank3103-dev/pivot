<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/_bg__iphone_14_plus___2_ek2_color"
    tools:context=".invoice">
    <RelativeLayout android:id="@+id/content_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <RelativeLayout android:id="@+id/page_login__ek1"
            android:layout_marginTop="0dp"
            android:layout_centerHorizontal="true"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <ImageView
                android:layout_width="wrap_content"
                android:background="@drawable/sucessfull"
                android:layout_centerHorizontal="true"
                android:id="@+id/image"
               android:layout_marginTop="100dp"
                android:layout_height="wrap_content">

            </ImageView>



            <RelativeLayout android:id="@+id/group_56"
                android:layout_marginTop="350dp"
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <RelativeLayout android:id="@+id/group_289"
                    android:layout_marginTop="0dp"
                    android:background="@drawable/border_shape"

                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:text="Order detail"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="10dp"
                        android:textStyle="bold"
                        android:id="@+id/textOrder"
                        android:textSize="15dp"
                        android:textColor="@color/black"
                        android:layout_height="wrap_content"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:text="Pricing Plan"
                        android:layout_below="@+id/textOrder"
                        android:layout_marginTop="50dp"
                        android:layout_marginStart="10dp"

                        android:textSize="15dp"
                        android:id="@+id/textOrder2"

                        android:layout_height="wrap_content"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:text="Premium 1 year"
                        android:layout_below="@+id/textOrder"
                        android:layout_marginTop="50dp"
                        android:layout_marginStart="300dp"

                        android:textSize="15dp"
                        android:id="@+id/premium"

                        android:layout_height="wrap_content"/>
                    <TextView
